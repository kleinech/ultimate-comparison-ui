{
  "name": "ultimate-comparison-ui",
  "productName": "Ultimate Comparison UI",
  "description": "UI for managing ultimate comparisons",
  "version": "2.0.0-alpha.1",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"ng serve --aot\" \"npm run electron\"",
    "ng": "ng",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron ./src/electron.dev",
    "package:win": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --platform=win32 --arch=all --overwrite --asar=true",
    "package:linux": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --platform=linux --arch=all --overwrite --asar=true",
    "package:osx": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --platform=darwin --arch=all --overwrite ",
    "package:all": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --all --arch=all --overwrite "
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:ultimate-comparisons/ultimate-comparison-ui.git"
  },
  "dependencies": {
    "@angular/animations": "6.0.7",
    "@angular/cdk": "^6.3.1",
    "@angular/common": "6.0.7",
    "@angular/compiler": "6.0.7",
    "@angular/core": "6.0.7",
    "@angular/forms": "6.0.7",
    "@angular/http": "6.0.7",
    "@angular/material": "^6.3.1",
    "@angular/platform-browser": "6.0.7",
    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/router": "6.0.7",
    "@types/js-yaml": "^3.11.1",
    "@types/prismjs": "^1.9.0",
    "@types/showdown": "^1.7.4",
    "@types/yamljs": "^0.2.30",
    "citation-js": "^0.4.0-5",
    "core-js": "^2.5.7",
    "fs": "0.0.1-security",
    "js-yaml": "^3.12.0",
    "material-community-components": "^3.2.1",
    "material-design-icons": "^3.0.1",
    "ng2-simplemde": "^2.0.2",
    "ngx-electron": "^1.0.4",
    "ngx-highlightjs": "^2.1.1",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "prismjs": "^1.15.0",
    "rxjs": "^6.2.1",
    "showdown": "^1.8.6",
    "yamljs": "^0.3.0",
    "zone.js": "^0.8.26",
    "cross-var": "^1.1.0",
    "electron-packager": "^12.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.7",
    "@angular/language-service": "6.0.7",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.113",
    "codelyzer": "^4.4.2",
    "concurrently": "^3.6.0",
    "electron": "^1.8.7",
    "electron-winstaller": "^2.6.4",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.9.2",
    "protractor": "^5.4.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.7.2"
  },
  "optionalDependencies": {
    "electron-installer-debian": "^0.8.1"
  }
}
